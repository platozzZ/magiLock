page{
  display:flex;
  display:-webkit-flex;
  height:100%;
  width:100%;
}
.detail{
    background: #fff;
}
.flex{
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    margin: 30px 0;
    font-size: 14px;
    padding: 0 20px;
}
.detail-image image{
    width: 100px;
    margin: 0 auto;
}
.detail-info{
    width: 150px;
    margin: 0 auto;
}
.detail-info>view{
    flex: 1;
    text-align: center;
}
.detail-info image{
    width: 20px;
}
.detail-btn{
    margin: 20px auto;
    width: 180px;
}
.detail-btn-btn{
    width: 70px;
    font-size: 12px;
    line-height: 26px;
    border: 1rpx solid #eee;
    border-radius: 5px;
    text-align: center;
}
.detail-btn-btn:last-child{
    margin-left: 40px;
}
.btn-delete{
    
}
.weui-cells__title{
    padding: 10px 15px 0;
    margin: 0;
    color: #353535;
}
.gateway{
    font-size: 13px;
    margin: 10px;
}
.gateway image{
    display: inline-block;
    width: 16px;
    vertical-align: -3px;
    margin-right: 5px;
}