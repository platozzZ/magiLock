<form bindsubmit="formSubmit">
    <view class='login-input'>
        <view class='login-input-cell'>
            <view class='label'>密码</view>
            <view class='input'><input type='text' name="mobile" placeholder=''></input></view>
        </view>
        <view class='login-input-cell'>
            <view class='label'>开始时间</view>
            <view class='input'><input name="reg"></input></view>
            <view class='time'><input name="reg"></input></view>
        </view>
        <view class='login-input-cell'>
            <view class='label'>结束时间</view>
            <view class='input'><input name="reg"></input></view>
            <view class='time'><input name="reg"></input></view>
        </view>
        <view class='login-input-cell' style='margin-top: 20px;'>
            <button class="weui-btn" type="primary" form-type="submit">确定</button>
        </view>
    </view>
</form>